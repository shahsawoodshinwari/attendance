{
    "name": "ianvizarra/attendance",
    "description": "Attendance package for Laravel application.",
    "keywords": [
        "ianvizarra",
        "laravel",
        "attendance",
        "package"
    ],
    "homepage": "https://github.com/ianvizarra/attendance",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Vizarra",
            "email": "ian.vizarra@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^12.0",
        "spatie/enum": "^3.13",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Ianvizarra\\Attendance\\": "src/",
            "Ianvizarra\\Attendance\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ianvizarra\\Attendance\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ianvizarra\\Attendance\\AttendanceServiceProvider"
            ],
            "aliases": {
                "Attendance": "Ianvizarra\\Attendance\\Facades\\Attendance"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
